#include<stdio.h>
int main()
{
    int mark;
    printf("enter marks :\n");
    scanf("%d",&mark);
    if(mark>84)
    {
        printf("Grade A");
    }
    else if(mark<=84 && 70<=mark)
    {
        printf("Grade B");
    }
    else if(mark<=69 && 55<=mark)
    {
        printf("Grade c");
    }
    else if(mark<=54 && 40<=mark)
    }
        printf("Grade D");
    }
    else if(mark<40)
    {
        printf("Grade F");
    }
}
